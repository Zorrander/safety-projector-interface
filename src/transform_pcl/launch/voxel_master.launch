<launch>
  <node pkg="nodelet" type="nodelet" name="pcl_manager" args="manager" output="screen" />

  <!-- Run a VoxelGrid filter to clean NaNs and downsample the data 
  <node pkg="nodelet" type="nodelet" name="voxel_grid_sub" args="load pcl/VoxelGrid pcl_manager" output="screen">
    <remap from="~input" to="/sub/points2" />
    <rosparam>
      filter_field_name: z
      filter_limit_min: 2.6
      filter_limit_max: 4.0
      filter_limit_negative: False
      leaf_size: 0.1
    </rosparam>
  </node>-->
  <node pkg="nodelet" type="nodelet" name="voxel_grid_master" args="load pcl/VoxelGrid pcl_manager" output="screen">
    <remap from="~input" to="/master/points2" />
    <rosparam>
      filter_field_name: z
      filter_limit_min: 2.5
      filter_limit_max: 4.0
      filter_limit_negative: False
      leaf_size: 0.005
    </rosparam>
  </node>

  <!-- Run a Statistical Ouliers Removal 
  <node pkg="nodelet" type="nodelet" name="stat_outliers_sub" args="load pcl/StatisticalOutlierRemoval pcl_manager" output="screen">
    <remap from="~input" to="/voxel_grid_sub/ouput" />
    <rosparam>
      mean_k: 50
      stddev: 1.0
      negative: False
    </rosparam>
  </node>
  <node pkg="nodelet" type="nodelet" name="stat_outliers_master" args="load pcl/StatisticalOutlierRemoval pcl_manager" output="screen">
    <remap from="~input" to="/voxel_grid_master/ouput" />
    <rosparam>
      mean_k: 50
      stddev: 1.0
      negative: False
    </rosparam>
  </node>-->
</launch>